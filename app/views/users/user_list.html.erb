<h1><%= @title %></h1>


<%= will_paginate %>

<div class="row">
<% @users.each do |u| %>
	 <div class="col-xs-6 col-md-3">

        <div class="thumbnail">
        <%= link_to image_tag(u.profile_photo.url(:medium)), user_path(u) %>
        <dt><%= link_to u.name, user_path(u)%></dt>
        </div>    
        <div class="clearfix visible-xs-block"></div>
    	</div>
<% end %>
</div>

<%= will_paginate %>




<br>
